{
  "name": "express-typescript-app",
  "version": "1.0.0",
  "description": "Express app with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "init": "git submodule update --init --recursive && npm install",
    "submodule": "git submodule update --remote --recursive",
    "typecheck": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "express",
    "typescript",
    "nodemon",
    "colors"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "colors": "1.4.0",
    "dotenv": "^17.2.3",
    "express": "4.18.2",
    "morgan": "^1.10.1",
    "odbc": "^2.4.9",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/express": "4.17.17",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.15.5",
    "cross-env": "10.1.0",
    "nodemon": "3.0.1",
    "rimraf": "5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  }
}


